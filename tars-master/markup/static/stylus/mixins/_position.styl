abs(d,w,h,t,l)
    position absolute 
    display d
    top t
    left l
    width w
    height h
    
    
t-cell(n)
    position relative
    display table-cell
    vertical-align n
    
    
float-w(n)
    float left
    width n
    
    
// Center-align a block level element
center-block()
  display block
  margin-left auto
  margin-right auto

    
